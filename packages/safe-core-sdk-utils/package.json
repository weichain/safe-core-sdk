  "devDependencies": {
    "@weichain/safe-deployments": "1.17.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.9",
    "@types/semver": "^7.3.13",
